{
    "name": "ivent-client",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.20.0",
        "react-scripts": "^5.0.1",
        "styled-components": "^6.1.1",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "commitlint": "npx commitlint --edit",
        "lint-check": "npx eslint .",
        "lint-fix": "npx eslint . --fix",
        "prettier-check": "npx prettier --check \"src/**/*.+(js|ts|json)\"",
        "prettier-fix": "npx prettier --write \"src/**/*.+(js|ts|json)\"",
        "postinstall": "npx patch-package && npx husky install",
        "pre-commit": "npm run lint-check && npx lint-staged && rm -rf ./build ./docs && npm run build && mv ./build ./docs && git add ./docs"
    },
    "lint-staged": {
        "**/*.{ts,tsx,js,jsx}": [
            "npm run prettier-fix",
            "npm run lint-fix"
        ]
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.12.0",
        "commitlint": "^18.4.3",
        "conventional-changelog-conventionalcommits": "^7.0.2",
        "eslint": "^8.54.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "husky": "^8.0.3",
        "install": "^0.13.0",
        "npm": "^10.2.4",
        "prettier": "^3.1.0"
    }
}
